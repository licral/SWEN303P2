<!DOCTYPE html>
<html>
	<head>
		<title><%= title %> - <%= page %></title>
		<link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css'/>
		<link rel='stylesheet' href='../stylesheets/style.css'/>
	</head>
	<body>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

	<!-- Creates the menubar as defined in menubar-src -->
	<% include include/menubar.ejs %>


    <!--Upload some images via sell then check this out! -->
	<div class="container">

		<div class="row">

			<div class="col-lg-12" id = header>
				<h1 class="page-header">Technology Listings</h1>
			</div>

			<!-- Page Content -->
			<ul class="breadcrumb">
				<li><a href="#">Home</a></li>
				<li><a href="#">Private</a></li>
				<li><a href="#">Pictures</a></li>
				<li class="active">Vacation</li>
			</ul>

            <% items.forEach(function(item) { %>
            <div class="col-lg-3 col-md-4 col-xs-12 thumb" id = entry>
                <p class = item><%= item.title %></p>
                <a class="entry" href="item/<%= item._id %>">
                    <img class="img-responsive" src="image/<%= item._id %>" alt="">
                </a>
                <p class = stock>In Stock: <%= item.stock %></p>
                <p class =price>Price: $<%= item.price %></p>
            </div>
            <% }); %>



		</div>

		<ul class="pagination">
			<li><a href="#">1</a></li>
			<li><a href="#">2</a></li>
			<li><a href="#">3</a></li>
			<li><a href="#">4</a></li>
			<li><a href="#">5</a></li>
		</ul>


    </div>
	</body>
</html>